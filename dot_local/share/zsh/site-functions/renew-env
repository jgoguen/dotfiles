# Do not edit this file directly! It is managed by chezmoi.
emulate -L zsh

if [ -n "${TMUX}" ]; then
	for v in ${@}; do
		exp="$(tmux show-environment | grep "^${v}")"
		if [ -n "${exp}" ]; then
			export "${exp?}"
		fi
	done
fi
