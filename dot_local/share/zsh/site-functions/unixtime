# Do not edit this file directly! It is managed by chezmoi.
emulate -L zsh

if [ -n "${1}" ]; then
	printf '%s' "${1}" | awk '{ printf("%s", strftime("%Y-%b-%d %H:%M:%S\n", $1)); }'
else
	date +%s
fi
