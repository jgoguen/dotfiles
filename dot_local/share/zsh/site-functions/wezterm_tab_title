emulate -L zsh

if [ -z "${TMUX}" ]; then
	printf '\x1b]1337;SetUserVar=panetitle=%s\x07' "$(printf "%s" "${1}" | base64)"
else
	printf '\x1bPtmux;\x1b\x1b]1337;SetUserVar=panetitle=%s\x07\x1b\\' "$(printf "%s" "${1}" | base64)"
fi
